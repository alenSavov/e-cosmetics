}

<section class="site-wrapper">
    <aside class="aside">
        @*<p class="link-fb"><a href="https://www.facebook.com">FACEBOOK</a></p>
        <p class="link-twitter"><a href="https://www.facebook.com">TWITTER</a></p>
        <p class="link-instagram"><a href="https://www.facebook.com">INSTAGRAM</a></p>*@
        <ul>
            <li><p>beauty</p></li>
            <li><p>about</p></li>
            <li><p>truth</p></li>
            <li><p>The</p></li>
        </ul>
    </aside>

    <section class="site-banner">

        <!-- Slideshow container -->
        <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img class="d-block w-100" src="~/img/model4.jpg" alt="First slide">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="~/img//banner.jpg" alt="Second slide">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="~/img/model5.jpg" alt="Third slide">
                </div>
            </div>
            <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>

    </section>
</section>